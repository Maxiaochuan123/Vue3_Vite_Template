{
  "name": "test-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite serve --mode development",
    "test": "vite serve --mode test",
    "prod": "vite serve --mode production",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "eslint": "eslint --ext .js,.ts,.vue,.jsx,.tsx --fix src",
    "prettier": "prettier --write .",
    "preview": "vite preview",
    "commit": "git add . && git-cz",
    "scanner": "i18next-scanner --config ./src/i18n/scanner.config.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^9.2.0",
    "axios": "0.27.2",
    "currency.js": "^2.0.4",
    "element-plus": "^2.2.17",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "normalize.css": "^8.0.1",
    "nprogress": "0.2.0",
    "pinia": "^2.0.22",
    "pinia-plugin-persistedstate": "^2.2.0",
    "qs": "^6.11.0",
    "vue": "^3.2.39",
    "vue-i18n": "9.3.0-beta.3",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.5.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.14",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "conventional-changelog-cli": "^2.2.2",
    "crc": "^4.1.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "i18next-scanner": "^4.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": ">=2.79.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-visualizer": "^5.8.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.22.4",
    "vite": "3.0.9",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vue-content-loader": "^2.0.1",
    "vue-tsc": "^0.40.5",
    "webpack": ">=5.74.0"
  }
}
